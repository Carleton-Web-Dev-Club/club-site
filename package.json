{
  "name": "carleton-web-dev-club-api",
  "version": "0.1.0",
  "description": "RESTful API for Carleton Web Dev Club",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:server",
    "start:server": "node -r esm server.js",
    "dev": "npm run start:mongo && nodemon -L --exec npm run start:server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "start:mongo": "cd mongodb && docker-compose up -d",
    "stop:mongo": "cd mongodb && docker-compose down",
    "install:docker": "cd mongodb && docker-compose pull",
    "docs": "docsify serve docs"
  },
  "author": "https://github.com/Carleton-Web-Dev-Club/club-site-api/graphs/contributors",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Carleton-Web-Dev-Club/club-site-api"
  },
  "bugs": {
    "url": "https://github.com/Carleton-Web-Dev-Club/club-site-api/issues"
  },
  "homepage": "https://carleton-web-dev-club.github.io/club-site-api/",
  "devDependencies": {
    "docsify-cli": "^4.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "mongoose": "^5.9.20",
    "mongoose-type-email": "^1.0.12",
    "morgan": "^1.10.0"
  }
